        "/auth/login": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Authenticate user and generate JWT token",
                "operationId": "2d1da45c2eeec3d321e5d37d471f109d",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "example": "admin"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "MaNs123456"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Login successful",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "profile": {
                                            "properties": {
                                                "name": {
                                                    "type": "string",
                                                    "example": "مستر محمد عبده"
                                                },
                                                "username": {
                                                    "type": "string",
                                                    "example": "Admin"
                                                },
                                                "phone": {
                                                    "type": "string",
                                                    "example": "1234567890"
                                                },
                                                "email": {
                                                    "type": "string",
                                                    "example": "admin@admin.com"
                                                }
                                            },
                                            "type": "object"
                                        },
                                        "role": {
                                            "type": "string",
                                            "example": "مدير عام"
                                        },
                                        "permissions": {
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "permissionName": {
                                                        "type": "string",
                                                        "example": "all_users"
                                                    },
                                                    "access": {
                                                        "type": "boolean",
                                                        "example": true
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        },
                                        "tokenDetails": {
                                            "properties": {
                                                "token": {
                                                    "type": "string",
                                                    "example": "21|2ceHorzqQmn3..."
                                                },
                                                "expiresIn": {
                                                    "type": "integer",
                                                    "example": 600
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Invalid credentials"
                    }
                }
            }
        },
        "/auth/logout": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Logout user",
                "operationId": "0ba0c27a94debd8532d78c5a92d93cd4",
                "responses": {
                    "200": {
                        "description": "Logout successful",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "You have logged out"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        }
    },
